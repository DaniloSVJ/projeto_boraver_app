{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
   "strict":true,
   "target": "ES3",
   "outDir": "jsScripts/",
   "sourceMap": false,
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "./src/*"
      ],
      
      "@theme/*": [
        "./src/theme/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
    }
  },
}
